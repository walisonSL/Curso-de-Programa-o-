//imprimir numeros impares at√© o numero escolhido
import java.util.Scanner;
public class Main
{
    public static void main(String args[]){
        int x = 0;
        Scanner sc = new Scanner(System.in);
        System.out.println("digite um numero");
        x = sc.nextInt();
        for ( int i = 1; i < x; i += 2){
            System.out.println(i);
	    } 
	    sc.close();
    }
}
