import java.util.Scanner;
public class Main{
	public static void main(String[] args){
	    System.out.println("digite um numero");
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		int fat = 1;
		for (int i=1; i<n; i++) {
		fat =fat * i;
		}
		System.out.println( "o fatorial de " + n + " Ã© " + n * fat);
		sc.close();
	}
}
